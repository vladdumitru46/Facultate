<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.reteasocialagui.UserView">
    <children>
        <Button fx:id="friendRequestButton" layoutX="372.0" layoutY="35.0" mnemonicParsing="false" onAction="#onFriendRequestPush" prefHeight="48.0" prefWidth="98.0" text="Friend requests" />
        <Button fx:id="addFriendButton" layoutX="372.0" layoutY="105.0" mnemonicParsing="false" onAction="#onAddFriendPush" prefHeight="48.0" prefWidth="93.0" text="Add Friend" />
        <Button fx:id="deleteFriendButton" layoutX="372.0" layoutY="176.0" mnemonicParsing="false" onAction="#onDeleteFriendPush" prefHeight="48.0" prefWidth="93.0" text="Delete friend" />
      <ListView fx:id="listView" layoutX="14.0" layoutY="17.0" prefHeight="382.0" prefWidth="344.0" />
      <Button fx:id="sendtRequestsButton" layoutX="374.0" layoutY="245.0" mnemonicParsing="false" onAction="#onSentRequestsPush" prefHeight="48.0" prefWidth="149.0" text="Sent Friend Requests" />
      <Button fx:id="chatButton" layoutX="375.0" layoutY="318.0" mnemonicParsing="false" onAction="#onChatPush" prefHeight="48.0" prefWidth="93.0" text="Chat" />
    </children>
</AnchorPane>
